xapiVersion: secrets-store.csi.x-k8s.io/v1
kind: SecretProviderClass
metadata:
  name: sketchydraw-aws-secrets
  namespace: default
spec:
  provider: aws
  parameters:
    objects: |
      - objectName: "sketchydraw/backend"
        objectType: "secretsmanager"
  secretObjects:
  - secretName: "sketchydraw-db-uri"
    type: Opaque
    data:
    - key: "DB_URI"
      objectName: "sketchydraw/backend"
